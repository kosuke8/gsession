<?xml version="1.0" encoding="Shift_jis" ?>

<plugin>

  <!-- プラグインID -->
  <id>dba</id>

  <!-- 名称  -->
  <name>DBA</name>

  <!-- 説明  -->
  <description>
GroupSessionで使用するデータベースをSQLの発行等によって直接メンテナンス可能にすること。
  </description>

  <!-- トップメニュー トップメニューの情報を記述する 記述しない場合はメニューに表示しない  -->
  <topmenu-info>
    <!-- メニューへの表示/非表示 -->
    <view>true</view>
    <!-- メニューアイコンクリック時に遷移するURL -->
    <url>../dba/dba001.do</url>
  </topmenu-info>

  <!-- メイン画面表示 メイン画面に表示する情報を記述する -->
  <mainscreen-info>
    <!-- メイン画面への表示/非表示 未設定の場合は表示しない -->
    <view>false</view>
  </mainscreen-info>

  <!-- ヘルプ情報を設定する。 -->
  <help-info>
    <!-- メニューの表示/非表示 -->
    <view>false</view>
  </help-info>

</plugin>